				"source": [
					"%%pyspark\r\n",
					"blob_account_name = \"pandemicdatalake\"\r\n",
					"blob_container_name = \"public\"\r\n",
					"blob_relative_path = \"curated/covid-19/ecdc_cases/latest/ecdc_cases.parquet\"\r\n",
					"blob_sas_token = r\"\"\r\n",
					"# Allow SPARK to read from Blob remotely\r\n",
					"wasbs_path = 'wasbs://%s@%s.blob.core.windows.net/%s' % (blob_container_name, blob_account_name, blob_relative_path)\r\n",
					"\r\n",
					"spark.conf.set(\r\n",
					"    'fs.azure.sas.%s.%s.blob.core.windows.net' % (blob_container_name, blob_account_name),\r\n",
					"    blob_sas_token)\r\n",
					"df = spark.read.parquet(wasbs_path)"
				],
				"execution_count": 1
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"df2 = df.select('year', 'month', 'cases', 'deaths')\r\n",
					"df3 = df2.groupBy('year', 'month').sum('cases', 'deaths')\r\n",
					"df_excercise_1 = df3.orderBy('year', 'month')\r\n",
					"display(df_excercise_1.limit(20))"
				],
				"execution_count": 2
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
						"outputs_hidden": false
					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					},
					"collapsed": false
				},
				"source": [
					"df2 = df.select('iso_country', 'cases', 'deaths')\r\n",
					"df3 = df2.groupBy('iso_country').sum('cases', 'deaths') \\\r\n",
					"    .withColumnRenamed(\"sum(cases)\", \"sum_cases\") \\\r\n",
					"    .withColumnRenamed(\"sum(deaths)\", \"sum_deaths\")\r\n",
					"df_excercise_2 = df3.orderBy('sum_cases', ascending=False)\r\n",
					"display(df_excercise_2.limit(10))"
				],
				"execution_count": 3
			}
		]
